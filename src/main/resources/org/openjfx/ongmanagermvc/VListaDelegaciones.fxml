<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="360.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.openjfx.ongmanagermvc.VListaDelegacionesControlador">
   <children>
      <TableView fx:id="table" layoutX="30.0" layoutY="26.0" prefHeight="252.0" prefWidth="440.0">
        <columns>
          <TableColumn fx:id="idDelegacion" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="nombre" prefWidth="75.0" text="Nombre" />
            <TableColumn fx:id="direccion" prefWidth="119.0" text="Dirección" />
            <TableColumn fx:id="telefono" prefWidth="76.0" text="Teléfono" />
            <TableColumn fx:id="sedeCentral" minWidth="5.0" prefWidth="75.0" text="Sede central" />
        </columns>
      </TableView>
      <Button fx:id="botonEditar" layoutX="125.0" layoutY="298.0" mnemonicParsing="false" onAction="#editarDelegacion" prefHeight="40.0" prefWidth="100.0" text="Editar" AnchorPane.leftAnchor="125.0" />
      <Button fx:id="botonCancelar" layoutX="275.0" layoutY="298.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="40.0" prefWidth="100.0" text="Cancelar" AnchorPane.rightAnchor="125.0" />
   </children>
</AnchorPane>
