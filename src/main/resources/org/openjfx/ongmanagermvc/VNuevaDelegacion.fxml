      </ImageView>
      <TextField fx:id="campoNombre" layoutX="126.0" layoutY="183.0" />
      <TextField fx:id="campoDireccion" layoutX="126.0" layoutY="225.0" />
      <TextField fx:id="campoTelefono" layoutX="126.0" layoutY="269.0" />
      <ComboBox fx:id="campoSedeCentral" layoutX="126.0" layoutY="312.0" prefWidth="150.0" />
      <Label alignment="CENTER_RIGHT" layoutX="37.0" layoutY="187.0" prefHeight="17.0" prefWidth="78.0" text="Nombre" />
      <Label layoutX="126.0" layoutY="146.0" prefHeight="27.0" prefWidth="241.0" text="Dar de alta delegación">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="37.0" layoutY="229.0" prefHeight="17.0" prefWidth="78.0" text="Dirección" />
